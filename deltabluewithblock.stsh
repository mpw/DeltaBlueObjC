#!/usr/local/bin/stsh
framework:DeltaBlue load.
c := 0.
f := 5.
k := 5.

solver := DBSolver solver.
c2f := solver constraintWithSTBlock:[  f := (9.0/5.0)*c + 32 ].
c2f add1ArgBlock:[ :farg | farg -32 * 5.0/9.0 ].

c2k := solver constraintWithSTBlock:[  k := c + 273.15 ].
c2k add1ArgBlock:[ :karg | karg - 273.15 ].

shell runInteractiveLoop.
